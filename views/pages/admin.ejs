<!DOCTYPE html>
<html lang="en">
    <%- include('../partials/head', { title: 'Main page' }) %>
<body>
    <%- include('../partials/header') %>
    <h1>admin page</h1>
    <div class="container">
        <h1>User Page</h1>
        <div class="card-deck">
            <% arr = arr.map(function(elem) {
            return '<div class="card"><h5 class="card-title">' + elem.name + '</h5> <h6 class="card-subtitle mb-2 text-muted">' + "location: " + elem.location + '</h6><div class="card-text"><p>'+ "Floor(s): "+elem.floor +'</p><p>'+ "Bedrooms: " + elem.bedrooms +'</p><p>'+ "Car spaces: "+ elem.car_spaces +'</p><p>'+ "Living spaces: " + elem.living_spaces +'</p><p>'+ "Bathrooms: " + elem.bathrooms +'</p><p>'+ "Area: "+ elem.area+ " &#13217;" +'</p><p>'+ "date sell from: "+ elem.date_sell_from +'</p><p>'+ "living spaces: " + elem.living_spaces +'</p><p>'+ "Price: "+ elem.price +" $"+'</p><p>'+ "Status: "+ elem.status +'</p></div></div>';
        }) -%>
            <p><%- arr.join("  ") %></p>
        </div>
    </div>
    </div>
    <%- include('../partials/footer') %>
</body>
</html>